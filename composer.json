{
  "name": "rizkussef/laravel-sql-to-migration",
  "description": "A Laravel package to convert raw SQL CREATE TABLE statements into Laravel migrations automatically.",
  "keywords": [
    "laravel",
    "sql to migration",
    "laravel migration generator",
    "convert sql to laravel migration",
    "mysql to laravel migration",
    "php artisan migration generator"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Rizkussef\\LaravelSqlToMigration\\": "src/"
    }
  },
  "authors": [
    {
      "name": "RizkUssef",
      "email": "rizkusgt67@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rizkussef\\LaravelSqlToMigration\\Providers\\SqlToMigrationServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "suggest": {
    "illuminate/support": "For helper methods and improved Laravel integration"
  },
  "conflict": {
    "laravel/framework": "<9.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
