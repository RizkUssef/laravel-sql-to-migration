{
  "name": "rizkussef/laravel-sql-to-migration",
  "description": "Convert SQL schema into Laravel migrations",
  "keywords": [
    "laravel",
    "migration",
    "sql",
    "database",
    "converter",
    "artisan"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Rizkussef\\LaravelSqlToMigration\\": "src/"
    }
  },
  "authors": [
    {
      "name": "RizkUssef",
      "email": "rizkusgt67@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/console": "^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "suggest": {
    "illuminate/support": "For helper methods and improved Laravel integration"
  },
  "conflict": {
    "laravel/framework": "<9.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
